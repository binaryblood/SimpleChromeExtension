{
  "manifest_version": 3,
  "name": "Search Plugin",
  "version": "1.0",
  "description": "Search plugin with a sidebar and attractive UI",
  "permissions": ["activeTab", "storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
